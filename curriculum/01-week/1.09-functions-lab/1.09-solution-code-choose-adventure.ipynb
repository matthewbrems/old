    "assert whitespace_removal(['San Francisco   ', 'New York City  ', 'Baltimore', 'Columbus_is_a_real_place   ']) == ['San Francisco', 'New York City', 'Baltimore', 'Columbus_is_a_real_place']\n",
    "assert (pi_estimation(n) - 3.142) < 0.01"
   ]
